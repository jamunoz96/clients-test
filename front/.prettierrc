{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "rangeStart": 0,
  "endOfLine": "auto",
  "jsxBracketSameLine": true,
  "react/jsx-max-props-per-line": [
    1,
    {
      "when": "always"
    }
  ]
}
